<% let size = Object.keys(currencyList).length; %>
<form action="/register" method="post">
    <input name="username" placeholder="username" required/>
    <input name="password" type="password" placeholder="password" required/>
    <input name="salary" placeholder="your salary" required/>
    <p>
        <select size="<%= size %>" multiple name="preferred_currency_id" required>
            <% Object.keys(currencyList).forEach((key) => { %>
                <option value="<%= key %>"><%= currencyList[key] %></option>
            <% }); %>
        </select>
    </p>
    <button type="submit">Register</button>
</form>
